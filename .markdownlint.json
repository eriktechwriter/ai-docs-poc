{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": {
    "line_length": 100,
    "code_blocks": false,
    "tables": false,
    "headings": false
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!?"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD033": {
    "allowed_elements": [
      "a",
      "abbr",
      "acronym",
      "b",
      "br",
      "code",
      "dd",
      "del",
      "details",
      "div",
      "dl",
      "dt",
      "em",
      "h1",
      "h2",
      "h3",
      "h4",
      "h5",
      "h6",
      "i",
      "img",
      "ins",
      "kbd",
      "li",
      "ol",
      "p",
      "pre",
      "s",
      "span",
      "strong",
      "sub",
      "summary",
      "sup",
      "table",
      "tbody",
      "td",
      "tfoot",
      "th",
      "thead",
      "tr",
      "ul",
      "var",
      "ExpandableSection",
      "DynamicTable",
      "FeatureMatrix",
      "ReleaseNotes",
      "ApiReference",
      "CodeBlock",
      "Tabs",
      "TabItem"
    ]
  },
  "MD034": false,
  "MD036": {
    "punctuation": ".,;:!?。，；：！？"
  },
  "MD040": true,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  }
}