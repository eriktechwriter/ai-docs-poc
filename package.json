{
  "name": "ai-docs-poc",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Centric Docs-as-Code POC for Trend Vision One Documentation",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "convert:md-to-mdx": "node scripts/convert-md-to-mdx.js",
    "validate:content": "node scripts/validate-content.js",
    "ai:plan": "node scripts/ai-content-planner.js",
    "ai:generate": "node scripts/ai-content-generator.js",
    "quality:check": "npm run lint && npm run typecheck",
    "preview": "npm run build && npm run serve"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@docusaurus/theme-mermaid": "^3.1.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "yaml": "^2.3.4",
    "gray-matter": "^4.0.3",
    "remark-gfm": "^4.0.0",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.0",
    "@docusaurus/tsconfig": "^3.1.0",
    "@docusaurus/types": "^3.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "chalk": "^5.3.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "documentation",
    "ai",
    "docusaurus",
    "mdx",
    "technical-writing",
    "docs-as-code"
  ],
  "author": "Trend Micro Documentation Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/trendmicro/ai-docs-poc.git"
  }
}