{
  "name": "ai-docs-poc",
  "version": "0.1.0",
  "private": true,
  "description": "AI-Centric Docs-as-Code POC for Trend Vision One Documentation",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "convert:md-to-mdx": "node scripts/convert-md-to-mdx.js",
    "fix:mdx-conversion": "node scripts/fix-mdx-conversion.js",
    "cleanup:mdx": "node scripts/cleanup-mdx.js",
    "cleanup:mdx-dry": "node scripts/cleanup-mdx.js --dry-run",
    "validate:content": "node scripts/validate-content.js",
    "ai:plan": "node scripts/ai-content-planner.js",
    "ai:generate": "node scripts/generate-content.js",
    "ai:generate-only": "node scripts/ai-content-generator.js",
    "ai:optimize": "node scripts/content-optimizer.js",
    "ai:analyze": "node scripts/content-quality-analyzer.js",
    "ai:review": "node scripts/review-workflow.js",
    "ai:feedback": "node scripts/feedback-processor.js",
    "system:health": "node scripts/system-health-check.js",
    "quality:check": "npm run lint && npm run typecheck",
    "quality:spelling": "cspell docs/**/*.{md,mdx} --config .cspell.json",
    "quality:markdown": "markdownlint docs/**/*.{md,mdx} --config .markdownlint.json",
    "quality:prose": "vale docs/ --config .vale.ini",
    "quality:comprehensive": "node scripts/quality-check.js",
    "quality:all": "npm run quality:check && npm run quality:comprehensive",
    "preview": "npm run build && npm run serve",
    "dev": "node scripts/dev-server.js",
    "dev:basic": "npm start",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx}\"",
    "setup": "npm install && npm run clear && node scripts/setup-dev-env.js",
    "setup:env": "node scripts/setup-dev-env.js",
    "health": "node scripts/health-check.js",
    "clean": "npm run clear && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "toc:setup": "node scripts/setup-recommended-toc-tools.js",
    "toc:generate": "doctoc docs/ --github --title '**Table of Contents**'",
    "toc:update": "doctoc docs/ --update-only --github",
    "toc:markdown": "markdown-toc -i docs/**/*.md",
    "toc:validate": "node scripts/validate-sidebar.js",
    "toc:visual": "code scripts/toc-visual-editor.html",
    "toc:guide": "node scripts/setup-toc-extensions.js",
    "docs:index": "node scripts/enterprise-doc-indexer.js",
    "docs:index-stats": "node scripts/enterprise-doc-indexer.js --stats",
    "docs:context-test": "node scripts/enterprise-context-retriever.js"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@docusaurus/theme-mermaid": "^3.1.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.29.3",
    "docusaurus-plugin-auto-sidebars": "^1.0.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "mermaid": "^10.6.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.8.0",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.0",
    "@docusaurus/tsconfig": "^3.1.0",
    "@docusaurus/types": "^3.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "eslint": "^8.0.0",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "inquirer": "^9.2.12",
    "markdown-toc": "^1.2.0",
    "markdownlint-cli": "^0.37.0",
    "ora": "^6.3.1",
    "prettier": "^3.1.0",
    "remark-toc": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "documentation",
    "ai",
    "docusaurus",
    "mdx",
    "technical-writing",
    "docs-as-code"
  ],
  "author": "Trend Micro Documentation Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/trendmicro/ai-docs-poc.git"
  }
}
